<h1>Create Projects</h1>
<%= form_for @project do |f| %>

	<p>
    <%= f.label :name %>
    <%= f.text_field :name %>
  </p>

  <p>
    <%= f.label :start_date %>
    <%= f.date_field :start_date %>
  </p>

  <p>
    <%= f.label :end_date %>
    <%= f.date_field :end_date %>
  </p>

  <p>
    <%= f.submit "Create" %>
  </p>
  
<%end%>